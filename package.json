{
    "author": "mrodrig",
    "name": "doc-path",
    "description": "A document path library for Node",
    "version": "2.2.0",
    "repository": {
        "type": "git",
        "url": "http://github.com/mrodrig/doc-path.git"
    },
    "main": "./dist/path.js",
    "scripts": {
        "prepublishOnly": "npm run lint && npm run uglify && npm run test",
        "test": "mocha test/tests.js",
        "coverage": "nyc --reporter=lcov --reporter=text-summary --reporter=text _mocha",
        "lint": "eslint src test",
        "uglify": "uglifyjs --compress --mangle --comments --output dist/path.js -- src/path.js"
    },
    "keywords": [
        "document",
        "json",
        "json library",
        "document path",
        "doc-path",
        "doc path",
        "doc",
        "path",
        "json path",
        "path evaluator",
        "object",
        "object path",
        "parse",
        "parser"
    ],
    "dependencies": {},
    "devDependencies": {
        "uglify-es": "3.3.9",
        "eslint": "6.8.0",
        "mocha": "7.1.2",
        "nyc": "14.1.1",
        "should": "13.2.3"
    },
    "engines": {
        "node": ">=10"
    },
    "license": "MIT"
}
